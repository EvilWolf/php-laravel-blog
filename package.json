        "popper.js": "^1.16",
        "postcss": "^8.4.12",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.50.0",
        "sass-loader": "^12.6.0",
        "vue-template-compiler": "^2.6.14"
